{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "attach",
      "name": "Attach to Cypress Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "skipFiles": ["cypress_runner.js"]
    },
    {
      "name": "Debug",
      "type": "node",
      "request": "attach",
      "port": 58585,
      "restart": true,
      "sourceMaps": true,
      "timeout": 60000
    },
    {
      "name": "Launch localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://192.168.1.195:8081/execute",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
